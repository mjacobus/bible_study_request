<% @visitors.each do |visitor|  %>
  <p>
    Nome: <%= visitor.name %><br>
    Email: <%= visitor.email %><br>
    Telefone: <%= visitor.phone %><br>
    CID: <%= visitor.cid %><br>
    UUID: <%= visitor.uuid %><br>
    Data: <%= visitor.created_at %>
  </p>
  <pre>
    <%= visitor.message %>
  </pre>
  <hr>
<% end %>
